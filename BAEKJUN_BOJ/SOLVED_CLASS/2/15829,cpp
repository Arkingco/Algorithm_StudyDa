#include<iostream>
#include<algorithm>
#include<cstring>
#include<vector>
#include<string>
#include<math.h>
#include<cstdio>
using namespace std;

int main()
{
	cout.tie(0); cin.tie(0); ios::sync_with_stdio(0);

	int n;
	cin >> n;
	
	int result = 0;
	for (int i = 0; i < n; ++i) {
		char a;
		cin >> a;

		result = result + (int)((a - 96) * (pow(31, i)));
	}

	cout << result % 1234567891 << endl;
}
